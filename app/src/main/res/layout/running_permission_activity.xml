<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".module.mine.app.RunningPermissionActivity">

    <include layout="@layout/public_head" />

    <TextView
        android:id="@+id/tvRunning1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="24dp"
        android:layout_marginRight="12dp"
        android:text="@string/running_tip1"
        app:layout_constraintTop_toBottomOf="@+id/public_head_viewgroup" />

    <TextView
        android:id="@+id/tvRunning2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="24dp"
        android:layout_marginRight="12dp"
        android:text="@string/running_tip2"
        app:layout_constraintTop_toBottomOf="@+id/tvRunning1" />

    <TextView
        android:id="@+id/tvRunning3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="24dp"
        android:layout_marginRight="12dp"
        android:text="@string/running_tip3"
        app:layout_constraintTop_toBottomOf="@+id/tvRunning2" />

</androidx.constraintlayout.widget.ConstraintLayout>